OUTPUT_FORMAT(pei-i386)
ENTRY(core_entry)
SECTIONS
{
  . = 0xE0000000;

  . = ALIGN(0x1000);
  .text :
  {  
    *(.text.bootstrap)
    *(.text.interrupts)
    *(.text.main) 
    *(.text.*)    
    *(.text)      
  }

  . = ALIGN(0x1000);
  .data :
  {     
    *(.rodata.options)
    *(.rodata.*)
    *(.rodata)
    *(.data.*)
    *(.data)    
  }

  . = ALIGN(16);
  .bss :
  {    
    *(.bss)
  }

  /DISCARD/ : 
  {    
    *(.eh_frame)
    *(.comment)
    *(.note.GNU-stack)
    *(.note.gnu.property)
    *(.group)
    *(.debug*)
    *(.debug_line_str)
    *(.debug_line)
    *(.debug_info)
    *(.debug_aranges)
    *(.debug_abbrev)
    *(.debug_rnglists)
  }
}
