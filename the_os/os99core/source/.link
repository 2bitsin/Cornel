OUTPUT_FORMAT(pei-i386)
ENTRY(core_entry)
SECTIONS
{  __image_base__ = 0xe0000000;
  .text (__image_base__) :
  {  
    *(.text.bootstrap)
    *(.text.interrupts)
    *(.text.main) 
    *(.text.*)    
    *(.text)      
  }

  .data : ALIGN(0x1000)
  {     
    *(.rodata.options)
    *(.rodata.*)
    *(.rodata)
    *(.data.*)
    *(.data)    
  }

  
  .bss : ALIGN(0x1000)
  {    
    *(.bss)
  }

  /DISCARD/ : 
  {    
    *(.eh_frame)
    *(.comment)
    *(.note.GNU-stack)
    *(.note.gnu.property)
    *(.group)
    *(.debug*)
    *(.debug_line_str)
    *(.debug_line)
    *(.debug_info)
    *(.debug_aranges)
    *(.debug_abbrev)
    *(.debug_rnglists)
  }
}
